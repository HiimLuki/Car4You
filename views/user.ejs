					<li><a href="/Autos">Unsere Autos</a></li>
				</ul>
			</div>	
		</div>
	<form action ="User" method="post">
		<div style="background-color:grey; width:  750px; height: 750px; margin-left: 400px; margin-top: 100px;">
			<div align = "left" style="font-size:25px;">
				
				Name: <%= sessionDaten[1]%> <%= sessionDaten[2]%><br><br>

				Telefon: <%= sessionDaten[3]%><br>
				<div class="form-input">
					<input type="text" name="telefon"
					placeholder="Telefon">
				</div>

				Mail: <%= sessionDaten[4]%><br><br>

				Postleitzahl: <%= sessionDaten[5]%> <%= sessionDaten[6]%><br>
				<div class="form-input">
					<input type="text" name="plz"
					placeholder="Plz">
				</div>
				<div class="form-input">
					<input type="text" name="ort"
					placeholder="Ort">
				</div>

				Adresse: <%= sessionDaten[7]%> <%= sessionDaten[8]%><br>
				<div class="form-input">
					<input type="text" name="straße"
					placeholder="Straße">
				</div>
				<div class="form-input">
					<input type="text" name="hausnummer"
					placeholder="hausnummer">
				</div>

				Geburtsdatum: <%= sessionDaten[9]%> <%= sessionDaten[10]%> <%= sessionDaten[11]%><br><br>

				Passwort ändern:
				<div class="form-input">
					<input type="password" name="passwort"
					placeholder="Passwort">
				</div>
				
				<input type="submit" name="submit"
					value="Daten ändern" class="login">
			</div>
		</div>
	</form>
