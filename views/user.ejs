<body>
<div id="maindiv"> 
			<div id="navdiv">
				<ul>
				<logo><a href='/Startseite'><img src="img/logohd.png"/></a></logo>
					<drop><a href="#"><img src="img/dropdown.png"/></a></drop>
					<log><a href='/logout'><img src="img/logout.png"/></a></log>
					<log-m><a href="/logout"><img src="img/logoutmobile.png"/></a></log-m>
					<!--<log><a href="login"><img src="img/login.png"/></a></log>-->
					<!--<log-m><a href="login"><img src="img/loginmobile.png"/></a></log-m>-->
					<li><a href="user"><%= sessionDaten[1]%></a></li>
					<li><a href="/MyMap">Maps</a></li>
					<li><a href="/Kontakt">Kontakt</a></li>
					<li><a href="/Kontaktlog">Kontakt</a></li>
